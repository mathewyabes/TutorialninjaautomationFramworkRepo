<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests" thread-count="2">
	<listeners>
		<listener class-name="com.tutorialsninja.listener.Listeners"></listener>
	</listeners>
	<test name="Test in chrome">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="com.tutorialsninja_Testcases.Register" />
			<class name="com.tutorialsninja_Testcases.Login" />
		</classes>
	</test>
	<test name="Test in edge">
		<parameter name="browser" value="edge"></parameter>
		<classes>
			<class name="com.tutorialsninja_Testcases.Register" />
			<class name="com.tutorialsninja_Testcases.Login" />
		</classes>
	</test>

</suite>



